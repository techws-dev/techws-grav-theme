{% for page in social_pages %}
	{% if page.url|length %}
		<a href="{{ page.url }}" {{ page.title ? 'title="' ~ page.title ~ '"' }} target="_blank" rel="me" class="navbar-item {{link_class}}">
			<span class="icon">
				<i class="fa fa-2x fa-{{ page.icon }}"></i>
			</span>
		</a>
	{% endif %}
{% endfor %}